<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1900BCD4">

    <TextView
        android:id="@+id/textViewETResult"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="132dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textViewET"
        app:layout_constraintTop_toBottomOf="@+id/textViewSTResult" />

    <TextView
        android:id="@+id/textViewSTResult"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textViewETResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewST"
        app:layout_constraintTop_toBottomOf="@+id/textViewDateResult" />

    <TextView
        android:id="@+id/textViewDateResult"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textViewSTResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewDate"
        app:layout_constraintTop_toBottomOf="@+id/textViewLocResult" />

    <TextView
        android:id="@+id/textViewLocResult"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textViewDateResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewLocation"
        app:layout_constraintTop_toBottomOf="@+id/textViewNameResult" />

    <TextView
        android:id="@+id/textViewET"
        android:layout_width="89dp"
        android:layout_height="41dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="297dp"
        android:layout_marginBottom="70dp"
        android:text="End Time:"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewST" />

    <TextView
        android:id="@+id/textViewST"
        android:layout_width="89dp"
        android:layout_height="41dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="297dp"
        android:text="Start Time:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewET"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewDate" />

    <TextView
        android:id="@+id/textViewDate"
        android:layout_width="89dp"
        android:layout_height="41dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="297dp"
        android:text="Date:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewST"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewLocation" />

    <TextView
        android:id="@+id/textViewLocation"
        android:layout_width="89dp"
        android:layout_height="41dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="297dp"
        android:text="Location:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewDate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewName" />

    <TextView
        android:id="@+id/textViewName"
        android:layout_width="89dp"
        android:layout_height="41dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="297dp"
        android:text="Name:"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewLocation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewNameResult"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="57dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textViewLocResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="146dp"
        android:layout_height="36dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="27dp"
        android:text="Event Details"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewNameResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonDone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="165dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="158dp"
        android:layout_marginBottom="63dp"
        android:background="#F5F7F4F4"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewETResult" />
</androidx.constraintlayout.widget.ConstraintLayout>